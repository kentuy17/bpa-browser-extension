{
  "name": "BrainPod Ai",
  "description": "AI Writer!",
  "version": "1.0",
  "manifest_version": 3,
  "background": {
    "service_worker": "background.js"
  },
  "permissions": ["storage", "activeTab", "scripting"],
  "action": {
    "default_popup": "popup.html",
    "default_icon": {
      "16": "/images/icon_temp16.png",
      "32": "/images/icon_temp32.png",
      "48": "/images/icon_temp48.png",
      "128": "/images/icon_temp128.png"
    },
    "icons": {
      "16": "/images/icon_temp16.png",
      "32": "/images/icon_temp32.png",
      "48": "/images/icon_temp48.png",
      "128": "/images/icon_temp128.png"
    }
  }
}